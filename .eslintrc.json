{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2
    },
    "rules": {
        "prop-types": 0,
        "comma-dangle": 0,
        "quotes": 0
    }
}